{
    "name": "larahmat/elasticquent",
    "description": "Connect to your Elasticsearch",
    "type": "library",
    "require": {
        "elasticsearch/elasticsearch": "^8.6",
        "php": "^8.0",
        "illuminate/container": "^10.4",
        "illuminate/contracts": "^10.4",
        "illuminate/support": "^10.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Larahmat\\Elasticquent\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rahmat Setiawan",
            "email": "setiawaneggy@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/eggysetiawan/elasticquent"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        },
        "sort-packages": true

    },
    "laravel": {
        "providers": [
            "Larahmat\\Elasticquent\\ElasticquentServiceProvider"
        ]
    }   
}
